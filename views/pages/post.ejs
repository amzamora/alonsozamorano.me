<%- include('../partials/header'); -%>

  <article class="post-page h-entry">
    <div class="wrapper">
      <header class="post-header">
        <h1 class="post-title p-name" itemprop="name headline"><%- post.title %></h1>
        <p class="post-meta">
          <time class="dt-published" datetime="<%- post.date %>" itemprop="datePublished">
            <%- post.date.toFormat('MMM d, yyyy') %>
          </time>
        </p>
      </header>

      <div class="post-content e-content" itemprop="articleBody">
        <%- post.content %>
      </div>
    </div>
  </article>

  <%- include('../partials/footer'); -%>